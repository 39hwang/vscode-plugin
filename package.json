{
    "name": "kite",
    "displayName": "kite",
    "description": "Python programming copilot â€” Kite shows cloud-powered completions, documentation, and examples.",
    "version": "0.26.0",
    "publisher": "kiteco",
    "engines": {
        "vscode": "^1.10.0"
    },
    "icon": "logo.png",
    "galleryBanner": {
        "color": "#ffffff",
        "theme": "light"
    },
    "author": {
        "name": "Kite"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/kiteco/vscode-plugin.git"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./src/kite",
    "contributes": {
        "commands": [
            {
                "command": "kite.search",
                "title": "Kite: Search"
            },
            {
                "command": "kite.reset-search-history",
                "title": "Kite: Reset Search History"
            },
            {
                "command": "kite.status",
                "title": "Kite: Status"
            },
            {
                "command": "kite.help",
                "title": "Kite: Help"
            },
            {
                "command": "kite.docs-for-cursor",
                "title": "Kite: Docs at cursor"
            },
            {
                "command": "kite.install",
                "title": "Kite: Install"
            },
            {
                "command": "kite.open-settings",
                "title": "Kite: Settings"
            },
            {
                "command": "kite.open-permissions",
                "title": "Kite: Permissions"
            },
            {
                "command": "kite.show-error-rescue",
                "title": "Kite: Open Error Rescue Sidebar"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "command": "kite.search"
                },
                {
                    "command": "kite.open-settings"
                },
                {
                    "command": "kite.open-permissions"
                },
                {
                    "command": "kite.docs-for-cursor"
                }
            ]
        },
        "configuration": {
            "type": "object",
            "title": "Kite Configuration",
            "properties": {
                "kite.showDocsNotificationOnStartup": {
                    "type": "boolean",
                    "default": true,
                    "description": "Whether to show the Kite docs notification on startup or not."
                },
                "kite.pollingInterval": {
                    "type": "integer",
                    "default": 5000
                },
                "kite.editorMetricsEnabled": {
                    "type": "string",
                    "default": "undefined",
                    "enum": [
                        "undefined",
                        "no",
                        "yes"
                    ]
                },
                "kite.sidebarDebugMode": {
                    "type": "boolean",
                    "default": false,
                    "description": "When enabled, each time a view is rendered in the kite sidebar the sample.html file will be updated with the sidebar content. It allows for quick inspection in a real browser."
                },
                "kite.developerMode": {
                    "type": "boolean",
                    "default": false,
                    "description": "Displays JSON data used by a view"
                },
                "kite.enableErrorRescue": {
                    "type": "boolean",
                    "default": true,
                    "description": "Enables error rescue feature on save."
                },
                "kite.actionWhenErrorRescueFixesCode": {
                    "type": "string",
                    "default": "Reopen sidebar",
                    "enum": [
                        "Reopen sidebar",
                        "Fix code quietly"
                    ],
                    "description": "Choose whether Error Rescue should (re)open its sidebar upon making fixes, or do it quietly"
                },
                "kite.loggingLevel": {
                    "type": "string",
                    "default": "info",
                    "enum": [
                        "silly",
                        "verbose",
                        "debug",
                        "info",
                        "warning",
                        "error"
                    ],
                    "description": "The verbosity level of Kite's logs."
                }
            }
        }
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "dependencies": {
        "analytics-node": "^3.1.1",
        "atob": "^2.0.3",
        "formidable": "^1.1.1",
        "getmac": "^1.2.1",
        "kite-installer": "^1.0.0",
        "md5": "^2.2.0",
        "opn": "^1.0.0",
        "rollbar": "^2.3.8",
        "tiny-relative-date": "^1.3.0"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "eslint": "^3.6.0",
        "expect.js": "^0.3.1",
        "jsdom": "^9.8.3",
        "mocha": "^2.3.3",
        "mocha-jsdom": "^1.1.0",
        "sinon": "^2.3.5",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "widjet-test-utils": "^1.8.0"
    }
}